<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pc.kaizer.netbank">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.pc.kaizer.netbank.WelcomeActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pc.kaizer.netbank.LoginActivity" android:label="NetBank Login"/>
        <activity android:name="com.pc.kaizer.netbank.RegisterActivity" android:label="NetBank Register"/>
        <activity android:name="com.pc.kaizer.netbank.MainActivity" android:label="Welcome to NetBank"/>
                <activity android:name="com.pc.kaizer.netbank.RegSuc" />
    </application>

</manifest>